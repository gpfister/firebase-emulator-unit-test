    "firebase-functions": "^3.16.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "typescript": "^4.5.5"
  },
  "private": true
}
